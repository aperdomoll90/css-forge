{
  "name": "css-forge",
  "title": "css-forge component library",
  "description": "React Component Library created by css-forge for experimental use",
  "version": "0.1.23",
  "license": "MIT",
  "author": "Adrian Perdomo Llerena",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "source": "index.tsx",
  "umd:main": "dist",
  "unpkg": "dist/index.js",
  "jsnext:main": "dist",
  "files": [
    "dist",
    "scripts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aperdomoll90/css-forge"
  },
  "private": false,
  "dependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:dist && yarn build:types",
    "build:dist": "rm -rf dist/ && rollup -c --bundleConfigAsCjs",
    "build:types": "node scripts/copy-types.js",
    "publish": "npm run build && npm publish --access public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "local": "node build.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-a11y": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.2",
    "@storybook/blocks": "^7.0.18",
    "@storybook/preset-create-react-app": "^7.0.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@swc/core": "^1.3.62",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.6",
    "@types/react": "^18.2.10",
    "@types/react-dom": "^18.2.4",
    "@types/three": "^0.152.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "css-loader": "^6.8.1",
    "gsap": "^3.12.1",
    "jest-axe": "^7.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.9.2",
    "postcss": "^8.4.24",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.1",
    "storybook": "^7.0.18",
    "style-loader": "^3.3.2",
    "three": "^0.153.0",
